{
    "name": "ClimateBase Crowd",
    "version": "1.0",
    "description": "Add organizations to ClimateBase's database!",
    "background": {
        "scripts": [
            "browser-polyfill.min.js",
            "jquery-3.4.1.min.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "images/logo48.png",
            "128": "images/logo128.png"
        }
    },
    "icons": {
        "48": "images/logo48.png",
        "128":"images/logo128.png"
    },
    "permissions": [
        "activeTab",
        "identity"
    ],
    "manifest_version": 2
  }
